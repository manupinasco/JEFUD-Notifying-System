<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/panelist_nav_graph"
    app:startDestination="@id/panelistAlarm">



    <fragment
        android:id="@+id/panelistMessageFr"
        android:name="ar.edu.ort.jefud_notifying_system.view.panelist.PanelistMessage"
        android:label="fragment_panelist_message"
        tools:layout="@layout/fragment_panelist_messages_received" >
        <action
            android:id="@+id/action_panelistMessageFr_to_panelistMessageSending"
            app:destination="@id/panelistMessageSending" />
        <action
            android:id="@+id/action_panelistMessageFr_to_panelistMessageDetails"
            app:destination="@id/panelistMessageDetails" />
    </fragment>
    <fragment
        android:id="@+id/panelistAlarm"
        android:name="ar.edu.ort.jefud_notifying_system.view.panelist.PanelistAlarm"
        android:label="fragment_panelist_alarm"
        tools:layout="@layout/fragment_panelist_alarm" />
    <fragment
        android:id="@+id/panelistMessageSending"
        android:name="ar.edu.ort.jefud_notifying_system.view.panelist.PanelistMessageSending"
        android:label="PanelistMessageSending" >
        <action
            android:id="@+id/action_panelistMessageSending_to_panelistMessageFr"
            app:destination="@id/panelistMessageFr"
            app:popUpTo="@id/panelistMessageFr"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/panelistMessageDetails"
        android:name="ar.edu.ort.jefud_notifying_system.view.panelist.PanelistMessageDetails"
        android:label="fragment_panelist_message_details"
        tools:layout="@layout/fragment_panelist_message_details" >
        <action
            android:id="@+id/action_panelistMessageDetails_to_panelistMessageFr"
            app:destination="@id/panelistMessageFr"
            app:popUpTo="@id/panelistMessageFr"
            app:popUpToInclusive="true" />
        <argument
            android:name="message"
            app:argType="ar.edu.ort.jefud_notifying_system.model.Message" />
        <argument
            android:name="user"
            app:argType="ar.edu.ort.jefud_notifying_system.model.User" />
    </fragment>
</navigation>